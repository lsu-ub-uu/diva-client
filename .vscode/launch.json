{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run dev",
      "name": "Dev server",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Attach debugger",
      "type": "node",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
