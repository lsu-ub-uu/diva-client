        },
        {
          "required": true,
          "type": "input",
          "regex": "^(\\d{4})-(\\d{4})-(\\d{4})-(\\d{3}[0-9X])$",
          "name": "ORCID",
          "label": {
            "sv": "ORCID-id"
          },
          "deftext": {
            "sv": "Avser personens unika ORCID-id"
