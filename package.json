{
  "name": "diva-client",
  "license": "GPL-3.0",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "npm run typegen && NODE_ENV=development tsx server.ts",
    "start": "tsx server.ts",
    "typegen": "tsx typegen/main.ts",
    "typecheck": "react-router typegen && tsc",
    "typecheck:w": "react-router typegen && tsc -w",
    "stylelint": "stylelint ./app/**/**/*.css",
    "lint": "eslint ./app/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./app/**/*.{ts,tsx}",
    "prettier-format": "prettier --write \"src/**/*.{ts,tsx,json}\" --config ./.prettierrc.json",
    "test": "vitest",
    "test:ci": "vitest run --reporter=junit --outputFile=./junit-report.xml",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "api-script": "tsx api-scripts/main.ts"
  },
  "engines": {
    "node": "22.14.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@hookform/resolvers": "^5.0.1",
    "@react-router/express": "^7.5.3",
    "@react-router/node": "^7.5.3",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "isbot": "^5.1.27",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.2",
    "react-i18next": "^15.5.1",
    "react-router": "^7.4.1",
    "remix-hook-form": "^7.0.1",
    "tsx": "^4.19.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.25.1",
    "@faker-js/faker": "^9.4.0",
    "@react-router/dev": "^7.5.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.11",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.1",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "eslint": "^9.26.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.2.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3",
