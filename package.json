{
  "name": "diva-client",
  "license": "GPL-3.0",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "BASE_PATH=/@BASENAME@ react-router build",
    "dev": "npm run typegen && NODE_ENV=development tsx server.ts",
    "prestart": "node scripts/prestart.js",
    "start": "npm run prestart && tsx server.ts",
    "typegen": "tsx typegen/main.ts",
    "typecheck": "react-router typegen && tsc",
    "typecheck:w": "react-router typegen && tsc -w",
    "stylelint": "stylelint ./app/**/**/*.{css,scss}",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier-format": "prettier --write \"src/**/*.{ts,tsx,json}\" --config ./.prettierrc.json",
    "test": "vitest --silent=true",
    "test:ci": "vitest run --reporter=junit --reporter=default --outputFile=./junit-report.xml",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage"
  },
  "engines": {
    "node": "22.14.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.1.1",
    "@react-router/express": "^7.13.0",
    "@react-router/node": "^7.13.0",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "dotenv": "^17.0.1",
    "express": "^5.2.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "isbot": "^5.1.31",
    "lodash-es": "^4.17.23",
    "lucide-react": "^0.563.0",
    "morgan": "^1.10.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^16.5.4",
    "react-router": "^7.9.1",
    "remix-hook-form": "^7.1.0",
    "sass": "^1.93.2",
    "tsx": "^4.20.6",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.2.0",
    "@react-router/dev": "^7.13.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.8",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.2.2",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.8",
    "vitest-mock-extended": "^3.1.0"
  }
}
